#!/usr/bin/env gnuplot

set terminal png

set xlabel "$t$, sec"
set ylabel "$\Delta t$, sec"

set logscale y
set output "delivery-p.png"

plot 'load-p.log' using ($2):($3) w d lt 2, \
     'stat-p.dat' using ($1):($2) w l lt 4, \
     'stat-p.dat' using ($1):($2):($3) w yerrorbars lt 4
#pause (-1)

unset logscale
set ylabel "messeges, sec$^{-1}$" 

set output "processed-p.png"

plot 'stat-p.dat' using ($1):($4) w l

#pause (-1)

